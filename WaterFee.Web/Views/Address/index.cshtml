        	$("#ffAdd").form("clear");
        	
        	url = '/Address/Insert';      

            //通过后台获取GUID并绑定
        	$.get("/Address/NewGuid?r=" + Math.random(), function (result) {
        	    $("#ID").val(result);
        	    ID = result;
        	});
        	$("#AddressType").val(addressTypeField);

        	var contactId = $("#ID").val();
