        //重新加载树形结构（异步）
        function reloadTree() {
            $("#loading").show();

            $('#treeDemo').tree({
                url: '/AddressGroup/GetGroupTree?addressType=' + addressType,
                onClick: function (node) {
                    loadData(node.id); //树单击节点操作
                }
            });

            InitDictItem();//同时刷新字典

            $("#loading").fadeOut(500);
        }

        //加载指定的对象数据
        var clickId = "";
        function loadData(id) {
            $("#loading").show();

            var queryParams = $('#grid').datagrid('options').queryParams;
            queryParams.CustomedCondition = id;

            $("#grid").datagrid("reload");
            $('#grid').datagrid('uncheckAll');

            clickId = id;

            $("#loading").fadeOut(500);
        }

