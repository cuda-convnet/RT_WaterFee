    <script type="text/javascript">
        var isAddOrEdit = 'add';//标识是新增还是编辑对话框弹出，用于删除附件的操作

        $(function () {

          
            var queryData = {

            };//可添加一些预设条件
            InitGrid(queryData);//初始化Datagrid表格数据
            //InitDictItem();   //初始化字典信息
            //价格属性
