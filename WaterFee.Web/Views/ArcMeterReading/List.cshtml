            var ID = $("#grid").datagrid('getSelections')[0].ID;
            //发送请求
            $.getJSON("/User/FindByID?r=" + Math.random() + "&id=" + ID, function (info) {
                $("#ID2").text(info.ID);
            });
        }

        //绑定修改按钮的事件
        function BindEditEvent() {
            $("#btnAddOK").unbind("click").click(Update);
        }

